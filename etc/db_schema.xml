        </index>
    </table>

    <!-- hans_office_employee_entity_varchar -->
    <table name="hans_office_employee_entity_varchar" resource="default" engine="innodb" comment="Hans Office Employee Varchar Attribute Backend Table">
        <column name="value_id" xsi:type="int" padding="11" unsigned="false" nullable="false" identity="true" comment="Value ID"/>
        <column name="attribute_id" xsi:type="smallint" padding="5" unsigned="true" nullable="false" identity="false" default="0" comment="Attribute ID"/>
        <column xsi:type="smallint" name="store_id" padding="5" unsigned="true" nullable="false" identity="false" default="0" comment="Store ID"/>
        <column xsi:type="smallint" name="entity_id" padding="6" unsigned="false" nullable="false" identity="false" default="0" comment="Entity ID"/>
        <column xsi:type="varchar" name="value" nullable="true" length="255" comment="Value"/>
        <constraint xsi:type="primary" referenceId="PRIMARY">
            <column name="value_id"/>
        </constraint>
        <constraint xsi:type="foreign" referenceId="OFF_EMPL_ENTT_VCHAR_ATTR_ID_EAV_ATTR_ATTR_ID" table="hans_office_employee_entity_varchar" column="attribute_id" referenceTable="eav_attribute" referenceColumn="attribute_id" onDelete="CASCADE"/>
        <constraint xsi:type="foreign" referenceId="OFF_EMPL_ENTT_VCHAR_ENTT_ID_OFF_EMPL_ENTT_ENTT_ID" table="hans_office_employee_entity_varchar" column="entity_id" referenceTable="hans_office_employee_entity" referenceColumn="entity_id" onDelete="CASCADE"/>
