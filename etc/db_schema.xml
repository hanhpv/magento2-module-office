<?xml version="1.0"?>
<schema xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:Setup/Declaration/Schema/etc/schema.xsd">
    <table name="hans_office_department" resource="default" engine="innodb" comment="Hans Office Department Table">
        <column xsi:type="smallint" name="entity_id" padding="6" unsigned="false" nullable="false" identity="true" comment="Entity ID"/>
        <column name="name" xsi:type="varchar" nullable="false" length="64" comment="Name"/>
        <constraint xsi:type="primary" referenceId="PRIMARY">
            <column name="entity_id"/>
        </constraint>
    </table>

    <!-- hans_office_employee_entity -->
    <table name="hans_office_employee_entity" resource="default" engine="innodb" comment="Hans Office Employee Table">
        <column name="entity_id" xsi:type="smallint" padding="6" unsigned="false" nullable="false" identity="true" comment="Entity ID"/>
        <column name="department_id" xsi:type="smallint" padding="6" unsigned="false" nullable="false" identity="false" comment="Department ID"/>
        <column name="email" xsi:type="varchar" length="64" comment="Email"/>
        <column name="firstname" xsi:type="varchar" length="64" comment="Firstname"/>
        <column name="lastname" xsi:type="varchar" length="64" comment="Lastname"/>
        <constraint xsi:type="primary" referenceId="PRIMARY">
            <column name="entity_id"/>
        </constraint>
        <constraint xsi:type="foreign" referenceId="HANS_OFFICE_EMPLOYEE_ENTITY_DEPARTMENT_ID_HANS_OFFICE_DEPARTMENT_ENTITY_ID" table="hans_office_employee_entity" column="department_id" referenceTable="hans_office_department" referenceColumn="entity_id"/>
    </table>

    <!-- hans_office_employee_entity_datetime -->
    <table name="hans_office_employee_entity_datetime" resource="default" engine="innodb" comment="Hans Office Employee Datetime Attribute Backend Table">
        <column name="value_id" xsi:type="int" padding="11" unsigned="false" nullable="false" identity="true" comment="Value ID"/>
        <column name="attribute_id" xsi:type="smallint" padding="5" unsigned="true" nullable="false" identity="false" default="0" comment="Attribute ID"/>
        <column xsi:type="smallint" name="store_id" padding="5" unsigned="true" nullable="false" identity="false" default="0" comment="Store ID"/>
        <column xsi:type="smallint" name="entity_id" padding="6" unsigned="false" nullable="false" identity="false" default="0" comment="Entity ID"/>
        <column xsi:type="datetime" name="value" on_update="false" nullable="true" comment="Value"/>
        <constraint xsi:type="primary" referenceId="PRIMARY">
            <column name="value_id"/>
        </constraint>
        <constraint xsi:type="foreign" referenceId="OFF_EMPL_ENTT_DTIME_ATTR_ID_EAV_ATTR_ATTR_ID" table="hans_office_employee_entity_datetime" column="attribute_id" referenceTable="eav_attribute" referenceColumn="attribute_id" onDelete="CASCADE"/>
        <constraint xsi:type="foreign" referenceId="OFF_EMPL_ENTT_DTIME_ENTT_ID_OFF_EMPL_ENTT_ENTT_ID" table="hans_office_employee_entity_datetime" column="entity_id" referenceTable="hans_office_employee_entity" referenceColumn="entity_id" onDelete="CASCADE"/>
        <constraint xsi:type="foreign" referenceId="OFF_EMPL_ENTT_DTIME_STORE_ID_STORE_STORE_ID" table="hans_office_employee_entity_datetime" column="store_id" referenceTable="store" referenceColumn="store_id" onDelete="CASCADE"/>
        <constraint xsi:type="unique" referenceId="OFF_EMPL_ENTT_DTIME_ENTITY_ID_ATTRIBUTE_ID_STORE_ID">
            <column name="entity_id"/>
            <column name="attribute_id"/>
            <column name="store_id"/>
        </constraint>
        <index referenceId="OFF_EMPL_ENTT_DTIME_ATTRIBUTE_ID" indexType="btree">
            <column name="attribute_id"/>
        </index>
        <index referenceId="OFF_EMPL_ENTT_DTIME_STORE_ID" indexType="btree">
            <column name="store_id"/>
        </index>
    </table>

    <!-- hans_office_employee_entity_decimal -->
    <table name="hans_office_employee_entity_decimal" resource="default" engine="innodb" comment="Hans Office Employee Decimal Attribute Backend Table">
        <column name="value_id" xsi:type="int" padding="11" unsigned="false" nullable="false" identity="true" comment="Value ID"/>
        <column name="attribute_id" xsi:type="smallint" padding="5" unsigned="true" nullable="false" identity="false" default="0" comment="Attribute ID"/>
        <column xsi:type="smallint" name="store_id" padding="5" unsigned="true" nullable="false" identity="false" default="0" comment="Store ID"/>
        <column xsi:type="smallint" name="entity_id" padding="6" unsigned="false" nullable="false" identity="false" default="0" comment="Entity ID"/>
        <column xsi:type="decimal" name="value" scale="4" precision="12" unsigned="false" nullable="true" comment="Value"/>
        <constraint xsi:type="primary" referenceId="PRIMARY">
            <column name="value_id"/>
        </constraint>
        <constraint xsi:type="foreign" referenceId="OFF_EMPL_ENTT_DECIM_ATTR_ID_EAV_ATTR_ATTR_ID" table="hans_office_employee_entity_decimal" column="attribute_id" referenceTable="eav_attribute" referenceColumn="attribute_id" onDelete="CASCADE"/>
        <constraint xsi:type="foreign" referenceId="OFF_EMPL_ENTT_DECIM_ENTT_ID_OFF_EMPL_ENTT_ENTT_ID" table="hans_office_employee_entity_decimal" column="entity_id" referenceTable="hans_office_employee_entity" referenceColumn="entity_id" onDelete="CASCADE"/>
        <constraint xsi:type="foreign" referenceId="OFF_EMPL_ENTT_DECIM_STORE_ID_STORE_STORE_ID" table="hans_office_employee_entity_decimal" column="store_id" referenceTable="store" referenceColumn="store_id" onDelete="CASCADE"/>
        <constraint xsi:type="unique" referenceId="OFF_EMPL_ENTT_DECIM_ENTITY_ID_ATTRIBUTE_ID_STORE_ID">
            <column name="entity_id"/>
            <column name="attribute_id"/>
            <column name="store_id"/>
        </constraint>
        <index referenceId="OFF_EMPL_ENTT_DECIM_ATTRIBUTE_ID" indexType="btree">
            <column name="attribute_id"/>
        </index>
        <index referenceId="OFF_EMPL_ENTT_DECIM_STORE_ID" indexType="btree">
            <column name="store_id"/>
        </index>
    </table>

    <!-- hans_office_employee_entity_int -->
    <table name="hans_office_employee_entity_int" resource="default" engine="innodb" comment="Hans Office Employee Integer Attribute Backend Table">
        <column name="value_id" xsi:type="int" padding="11" unsigned="false" nullable="false" identity="true" comment="Value ID"/>
        <column name="attribute_id" xsi:type="smallint" padding="5" unsigned="true" nullable="false" identity="false" default="0" comment="Attribute ID"/>
        <column xsi:type="smallint" name="store_id" padding="5" unsigned="true" nullable="false" identity="false" default="0" comment="Store ID"/>
        <column xsi:type="smallint" name="entity_id" padding="6" unsigned="false" nullable="false" identity="false" default="0" comment="Entity ID"/>
        <column xsi:type="int" name="value" padding="11" unsigned="false" nullable="true" identity="false" comment="Value"/>
        <constraint xsi:type="primary" referenceId="PRIMARY">
            <column name="value_id"/>
        </constraint>
        <constraint xsi:type="foreign" referenceId="OFF_EMPL_ENTT_INT_ATTR_ID_EAV_ATTR_ATTR_ID" table="hans_office_employee_entity_int" column="attribute_id" referenceTable="eav_attribute" referenceColumn="attribute_id" onDelete="CASCADE"/>
        <constraint xsi:type="foreign" referenceId="OFF_EMPL_ENTT_INT_ENTT_ID_OFF_EMPL_ENTT_ENTT_ID" table="hans_office_employee_entity_int" column="entity_id" referenceTable="hans_office_employee_entity" referenceColumn="entity_id" onDelete="CASCADE"/>
        <constraint xsi:type="foreign" referenceId="OFF_EMPL_ENTT_INT_STORE_ID_STORE_STORE_ID" table="hans_office_employee_entity_int" column="store_id" referenceTable="store" referenceColumn="store_id" onDelete="CASCADE"/>
        <constraint xsi:type="unique" referenceId="OFF_EMPL_ENTT_INT_ENTITY_ID_ATTRIBUTE_ID_STORE_ID">
            <column name="entity_id"/>
            <column name="attribute_id"/>
            <column name="store_id"/>
        </constraint>
        <index referenceId="OFF_EMPL_ENTT_INT_ATTRIBUTE_ID" indexType="btree">
            <column name="attribute_id"/>
        </index>
        <index referenceId="OFF_EMPL_ENTT_INT_STORE_ID" indexType="btree">
